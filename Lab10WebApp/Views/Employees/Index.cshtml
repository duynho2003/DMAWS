@model IEnumerable<Lab10WebApp.Models.Employee>
@{
    ViewBag.Title = "Index";
}
<h1>List of employee</h1>

<table class="table">
    <tr>
        <th>Code</th>
        <th>Name</th>
        <th>Phone</th>
        <th>Salary</th>
        <th>Roles</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Code</td>
            <td>@item.Name</td>
            <td>@item.Phone</td>
            <td>@item.Salary</td>
            <td>@item.Roles</td>
        </tr>
    }
</table>